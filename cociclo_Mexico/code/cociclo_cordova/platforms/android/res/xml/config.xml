<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.cociclo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AudioFrequency">
        <param name="android-package" value="com.cellules.cordova.audiofrequency.AudioFrequency" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <allow-intent href="market:*" />
    <name>CoCiiiclo</name>
    <description>
        Cocíclo permite de grabación y compartir datos de contaminación  
    </description>
    <author email="acastonguay@artengine.ca" href="https://github.com/alx-1/cociclo">
        Alexandre Castonguay
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
